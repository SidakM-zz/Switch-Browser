{
  "manifest_version": 2,

  "name": "Switch Browser",
  "description": "This extension opens all tabs currently open in chrome in some other browsers",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Switch Browser"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "background",
    "http://*/", "https://*/"
  ]

}